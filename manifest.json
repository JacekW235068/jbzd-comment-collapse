{

    "manifest_version": 2,
    "name": "Comment Wrap",
    "version": "1.0",
  
    "description": "Allows for hiding comment threads on jbzd.",

    "content_scripts": [
      {
        "matches": ["https://*.jbzd.com.pl/*"],
        "js": ["CommentWrap.js"],
        "css": ["CommentWrap.css"]
      }
    ],
    "web_accessible_resources": [
      "res/arrow.png"
    ]
}